{
  "name": "Screen sharing Extension",
  "description": "Screen sharing Extension for my app",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}